<?xml version="1.0" encoding="UTF-8"?><form><form_id>2:WPCD</form_id><name>Activity Template</name><form_type>1</form_type><data_type>0</data_type><form_data>&lt;html&gt;[lf]  &lt;head&gt;[lf]  &lt;/head&gt;[lf]  &lt;body&gt;[lf]  &lt;g:form action="worklist" method="post" &gt;[lf]    &lt;g:hiddenField name="iddb" value="${workInstance.activityInstID}" /&gt;[lf]    &lt;g:hiddenField name="iteration" value="${workInstance.workItemIteration}" /&gt;[lf]    &lt;g:hiddenField name="workRequest" value="${workRequest}" /&gt;[lf]    &lt;g:hiddenField name="procRefsRequested" value="${procRefsRequested}" /&gt;[lf]    &lt;div class="dialog"&gt;[lf]      &lt;table&gt;[lf]          &lt;tbody&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.job.id" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;&lt;g:link controller="job" action="show" params="[jobID:workInstance.jobID]"&gt;${fieldValue(bean: workInstance, field: "jobID")}&lt;/g:link&gt;&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.process.ref" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;&lt;g:link controller="process" action="processmap" params="[processRef:workInstance.processRef]"&gt;${fieldValue(bean: workInstance, field: "processRef")}&lt;/g:link&gt;&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.job.reference" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;${fieldValue(bean: workInstance, field: "jobRef")}&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.job.name" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;${fieldValue(bean: workInstance, field: "jobName")}&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.job.state" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;&lt;wp:formatJobState jobState="${workInstance?.jobState}" /&gt;&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.job.started" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;&lt;g:formatDate date="${workInstance?.jobStarted}" /&gt;&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.job.due" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;&lt;g:formatDate date="${workInstance?.jobDue}" /&gt;&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.priority" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;${fieldValue(bean: workInstance, field: "priorityName")}&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.activity.name" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;${fieldValue(bean: workInstance, field: "nodeName")}&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf][lf]              &lt;tr class="prop"&gt;[lf]                  &lt;td valign="top" class="name"&gt;&lt;g:message code="label.work.state" /&gt;&lt;/td&gt;[lf][lf]                  &lt;td valign="top" class="value"&gt;&lt;wp:formatWorkState workState="${workInstance?.workState}" /&gt;&lt;/td&gt;[lf][lf]              &lt;/tr&gt;[lf]              &lt;g:if test="${workInstance?.cclist}"&gt;  &lt;!-- Only display if there are completion codes to select --&gt;[lf]                  &lt;tr class="prop"&gt;[lf]                      &lt;td valign="top" class="name"&gt;[lf]                          &lt;label for="completionCode"&gt;&lt;g:message code="label.completion.code" /&gt;&lt;/label&gt;[lf]                      &lt;/td&gt;[lf]                      &lt;td valign="top" class="value"&gt;[lf]                          &lt;g:select name="completionCode" from="${workInstance?.cclist}" optionKey="completionCode" /&gt;[lf]                      &lt;/td&gt;[lf]                  &lt;/tr&gt;[lf]              &lt;/g:if&gt;[lf]              &lt;wp:showWorkUserData activityID="${workItem.activityInstID.toString()}" iteration="${workItem.activityIteration}"/&gt;[lf][lf]          &lt;/tbody&gt;[lf]      &lt;/table&gt;[lf]    &lt;/div&gt;[lf]    &lt;g:if test="${workInstance.workState == 4}"&gt;  &lt;!-- Open --&gt;[lf]        &lt;div class="buttons"&gt;[lf]            &lt;span class="button"&gt;[lf]                &lt;span class="button"&gt;&lt;g:actionSubmit value="${message(code:'label.complete')}" action="complete" /&gt;&lt;/span&gt;[lf]                &lt;span class="button"&gt;&lt;g:actionSubmit value="${message(code:'label.cancel')}" action="cancel" /&gt;&lt;/span&gt;[lf]                &lt;span class="button"&gt;&lt;g:actionSubmit value="${message(code:'label.suspend')}" action="suspend" /&gt;&lt;/span&gt;[lf]                &lt;span class="button"&gt;&lt;g:actionSubmit value="${message(code:'label.unopen')}" action="unopen" /&gt;&lt;/span&gt;[lf]            &lt;/span&gt;[lf]        &lt;/div&gt;[lf]    &lt;/g:if&gt;[lf]    &lt;g:else&gt;[lf]        &lt;g:if test="${workInstance.workState == 8}"&gt;  &lt;!-- Suspended --&gt;[lf]            &lt;div class="buttons"&gt;[lf]                &lt;span class="button"&gt;[lf]                    &lt;span class="button"&gt;&lt;g:actionSubmit value="${message(code:'label.open')}" action="open" /&gt;&lt;/span&gt;[lf]                    &lt;span class="button"&gt;&lt;g:actionSubmit value="${message(code:'label.complete')}" action="complete" /&gt;&lt;/span&gt;[lf]                    &lt;span class="button"&gt;&lt;g:actionSubmit value="${message(code:'label.cancel')}" action="cancel" /&gt;&lt;/span&gt;[lf]                &lt;/span&gt;[lf]            &lt;/div&gt;[lf]        &lt;/g:if&gt;[lf]    &lt;/g:else&gt;[lf]  &lt;/g:form&gt;[lf]  &lt;/body&gt;[lf][lf]&lt;/html&gt;</form_data><row_version>1</row_version><lu_id>Workpoint</lu_id><lu_date>2020-11-20T06:59:08</lu_date><validation_status>0</validation_status><validation_status_msg/><script_class_id>0</script_class_id></form>