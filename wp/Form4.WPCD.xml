<?xml version="1.0" encoding="UTF-8"?><form><form_id>4:WPCD</form_id><name>Work List Template</name><form_type>5</form_type><data_type>0</data_type><form_data>&lt;html&gt;[lf]&lt;head&gt;[lf]&lt;/head&gt;[lf]&lt;body&gt;[lf]&lt;div class="list"&gt;[lf]  &lt;table&gt;[lf]  &lt;thead&gt;[lf]    &lt;tr&gt;[lf]      &lt;th&gt;&lt;g:message code="label.activity" /&gt;&lt;/th&gt;[lf]        &lt;g:sortableColumn property="workState" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.work.state')}" /&gt;[lf]        &lt;g:sortableColumn property="jobID" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.job.id')}" /&gt;[lf]        &lt;g:sortableColumn property="jobRef" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.job.ref')}" /&gt;[lf]        &lt;g:sortableColumn property="jobState" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.job.state')}" /&gt;[lf]        &lt;g:sortableColumn property="jobStarted" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.job.started')}" /&gt;[lf]        &lt;th&gt;&lt;g:message code="label.job.due" /&gt;&lt;/th&gt;[lf]        &lt;g:sortableColumn property="plannedTime" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.work.planned.time')}" /&gt;[lf]        &lt;g:sortableColumn property="priority" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.priority')}" /&gt;[lf]        &lt;g:sortableColumn property="assignedTo" params="[workRequest:workRequest,procRefsRequested:procRefsRequested]" title="${message(code: 'label.assigned.to')}" /&gt;[lf]      &lt;/tr&gt;[lf]    &lt;/thead&gt;[lf]    &lt;tbody&gt;[lf]    &lt;g:each in="${workInstanceList}" status="i" var="workInstance"&gt;[lf]      &lt;tr class="${(i % 2) == 0 ? 'odd' : 'even'}"&gt;[lf]        &lt;td&gt;&lt;g:link action="show" params="[iddb:workInstance.activityInstID,iteration:workInstance.workItemIteration,workRequest:workRequest,procRefsRequested:procRefsRequested]"&gt;${fieldValue(bean: workInstance, field: "nodeName")}&lt;/g:link&gt;&lt;/td&gt;[lf]        &lt;td&gt;&lt;wp:formatWorkState workState="${workInstance.workState}" /&gt;&lt;/td&gt;[lf]        &lt;td&gt;${fieldValue(bean: workInstance, field: "jobID")}&lt;/td&gt;[lf]        &lt;td&gt;${fieldValue(bean: workInstance, field: "jobRef")}&lt;/td&gt;[lf]        &lt;td&gt;&lt;wp:formatJobState jobState="${workInstance.jobState}" /&gt;&lt;/td&gt;[lf]        &lt;td&gt;&lt;g:formatDate date="${workInstance.jobStarted}" type="datetime" style="MEDIUM"/&gt;&lt;/td&gt;[lf]        &lt;td&gt;&lt;g:formatDate date="${workInstance.jobDue}" type="datetime" style="MEDIUM"/&gt;&lt;/td&gt;[lf]        &lt;td&gt;&lt;wp:formatDuration duration="${workInstance.plannedTime}" /&gt;&lt;/td&gt;[lf]        &lt;td&gt;${fieldValue(bean: workInstance, field: "priorityName")}&lt;/td&gt;[lf]        &lt;td&gt;&lt;g:link action="reassign" params="[assignedTo:workInstance.assignedTo,jobID:workInstance.jobID,activityInstID:workInstance.activityInstID,iteration:workInstance.workItemIteration,nodeName:workInstance.nodeName,workRequest:workRequest,procRefsRequested:procRefsRequested]"&gt;${fieldValue(bean: workInstance, field: "assignedTo")}&lt;/g:link&gt;&lt;/td&gt;[lf]      &lt;/tr&gt;[lf]    &lt;/g:each&gt;[lf]    &lt;/tbody&gt;[lf]    &lt;/table&gt;[lf]&lt;/div&gt;[lf]&lt;/body&gt;[lf]&lt;/html&gt;[lf][lf]</form_data><row_version>1</row_version><lu_id>Workpoint</lu_id><lu_date>2020-11-20T06:59:08</lu_date><validation_status>0</validation_status><validation_status_msg/><script_class_id>0</script_class_id></form>